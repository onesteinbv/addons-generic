<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="application_stat_tree" model="ir.ui.view">
        <field name="model">argocd.application.stat</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="application_id" invisible="1" />
                <field name="date" />
                <field name="type_id" />
                <field name="value" />
            </tree>
        </field>
    </record>
</odoo>

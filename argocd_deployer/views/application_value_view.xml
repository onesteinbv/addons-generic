<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="application_value_tree" model="ir.ui.view">
        <field name="model">argocd.application.value</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="application_id" invisible="1" />
                <field name="key" />
                <field name="message" />
                <field name="value" />
            </tree>
        </field>
    </record>
</odoo>
